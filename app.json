{
  "expo": {
    "name": "Pledge",
    "slug": "clearmind-nofap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_fixed.png",
    "scheme": "nofapapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nofapapp.mobile",
      "buildNumber": "1",
      "deploymentTarget": "16.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses camera for profile pictures and companion interactions.",
        "NSUserTrackingUsageDescription": "This app uses tracking to provide personalized recovery insights and progress analytics."
      }
    },
    "android": {
      "package": "com.nofapapp.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_fixed.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "53e606fc-c01d-4c5c-8b72-89bb8fb60b3a"
      }
    },
    "owner": "abu2002"
  }
}
